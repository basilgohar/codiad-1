function heading() {
	
define( "", TRUE );
require_once( "./public/public.php" );
	?>
	</head>
	<body>
		<div id="mobile_menu_open" class="mobile_menu_open" onclick="openNav()">
			<a class="mobile_menu_open_button">&#9776; Menu</a>
		</div>
		
		<div id="main_menu_container" class="main_menu_container">
			<ul id="main_menu" class="main_menu">
				<li class="mobile_menu_close"><a class="mobile_menu_close_button" href="javascript:void(0)" onclick="closeNav()">&times;</a></li>
				<li class="main_menu_item <?php if ( $page == "index" || $page == "" ) { echo "menu_item_selected"; }?>"><a class="main_menu_link main_menu_link_selected" href="./index">Home</a></li>
				<li class="main_menu_item <?php if ( $page == "admin" ) { echo "menu_item_selected"; }?>"><a class="main_menu_link " href="./admin">Admin</a></li>
				<li class="main_menu_item <?php if ( $page == "students" ) { echo "menu_item_selected"; }?>"><a class="main_menu_link " href="./students">Students</a></li>
				<li class="main_menu_item <?php if ( $page == "quizzes" ) { echo "menu_item_selected"; }?>"><a class="main_menu_link " href="./quizzes">Quizzes</a></li>
			</ul>
		</div>
		<script>
			function openNav() {
				
				document.getElementById("main_menu").style.width = "250px";
			}

			function closeNav() {
				
				document.getElementById("main_menu").style.width = "0px";
			}
			// Not a ton of code, but hard to
			const nav = document.querySelector('#main_menu_container');
			const body = document.getElementById("main_menu_container");
			let topOfNav = nav.offsetTop;

			function fixNav() {
				if (window.scrollY >= topOfNav) {
					document.body.style.paddingTop = nav.offsetHeight + 'px';
					document.getElementById("main_menu_container").classList.add('main_menu_container_sticky');
				} else {
					document.getElementById("main_menu_container").classList.remove('main_menu_container_sticky');
					document.body.style.paddingTop = 0;
				}
			}

			window.addEventListener('scroll', fixNav);
		</script>
				
	<?php
} return( heading() );
?>";